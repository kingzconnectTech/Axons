{
  "expo": {
    "name": "Axon",
    "slug": "axon",
    "version": "1.0.0",
    "icon": "./assets/Axon.png",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Axon.png",
      "resizeMode": "contain",
      "backgroundColor": "#3E1E68"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.axon.techboi"
    },
    "android": {
      "package": "com.axon.techboi",
      "googleServicesFile": "./google-services.json",
      "icon": "./assets/Axon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/Axon.png",
        "backgroundColor": "#3E1E68"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "13ba9648-45b1-4723-a646-1291ddef18d9"
      }
    },
  "owner": "kingzconnect"
  }
}
