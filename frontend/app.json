{
  "expo": {
    "name": "Axon",
    "slug": "axon",
    "version": "1.0.0",
    "icon": "./assets/Axon.png",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/intro.gif",
      "resizeMode": "contain",
      "backgroundColor": "#3E1E68"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.axon.techboi"
    },
    "android": {
      "package": "com.axon.techboi",
      "googleServicesFile": "./google-services.json",
      "permissions": ["NOTIFIACTIONS"],
      "icon": "./assets/Axon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/Axon.png",
        "backgroundColor": "#3E1E68"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "sounds": [
            "./assets/axon_notification.mp3"
          ]
        }
      ],
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "13ba9648-45b1-4723-a646-1291ddef18d9"
      }
    },
    "owner": "kingzconnect"
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-4647863166992264~5710161013",
    "delay_app_measurement_init": false,
    "optimize_initialization": true,
    "optimize_ad_loading": true
  }
}
